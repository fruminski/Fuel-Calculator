generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Journey {
  id        Int    @id @default(autoincrement())
  start  String
  end String
  distance Float
  mpg Float
  fuelCost Float
  co2 Float
  createdAt DateTime @default(now())
}